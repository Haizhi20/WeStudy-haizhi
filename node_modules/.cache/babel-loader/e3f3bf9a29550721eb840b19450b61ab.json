;AAAA;AAAA;;AAAA;AAAA,OADA;AAoBA,IAAA,OAAA,EAAA,mBAAA;AACA,WAAA,KAAA,GAAA,KAAA,QAAA,KAAA,EAAA,GAAA,KAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,KAAA,QAAA,KAAA,EAAA,GAAA,KAAA,GAAA,IAAA;AACA,WAAA,OAAA,GAAA,KAAA,KAAA,KAAA,EAAA,GAAA,KAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,KAAA,QAAA,KAAA,EAAA,IAAA,KAAA,QAAA,IAAA,KAAA,QAAA,GAAA,KAAA,GAAA,IAAA;AACA,UAAA,KAAA,OAAA,IAAA,KAAA,KAAA,IAAA,KAAA,KAAA,IAAA,KAAA,MAAA,EACA,KAAA,IAAA,GAAA,IAAA,CADA,KAGA,KAAA,IAAA,GAAA,KAAA;AACA;AA7BA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div class=\"form\">\r\n          <p>注册你的账户</p>\r\n          <div class=\"jump\">\r\n              <span>已经有账户？</span>\r\n              <router-link to=\"/login\">立即登录</router-link>\r\n          </div>\r\n          <div class=\"container\">\r\n              <span :class=\"{warn:!eMailOK}\">方向</span>\r\n              <!-- <input type=\"text\" placeholder=\"请输入邮箱\" v-model=\"eMail\"> -->\r\n              <select v-model=\"select\">\r\n                  <option value=\"1\">前端</option>\r\n                  <option value=\"2\">后端</option>\r\n                  <option value=\"3\">人工智能</option>\r\n                  <option value=\"4\">产品</option>\r\n                  <option value=\"5\">嵌入式</option>\r\n                  <option value=\"6\">大数据</option>\r\n              </select>\r\n          </div>\r\n          <div class=\"container\">\r\n              <span :class=\"{warn:!usnOK}\">用户名</span>\r\n              <input type=\"text\" placeholder=\"请输入用户名\" v-model=\"username\">\r\n          </div>\r\n          <div class=\"container\">\r\n              <span :class=\"{warn:!pswOK}\">密码</span>\r\n              <input type=\"password\" placeholder=\"请输入密码\" v-model=\"password\">\r\n          </div>\r\n          <div class=\"container\">\r\n              <span :class=\"{warn:!cpswOK}\">确认密码</span>\r\n              <input type=\"password\" placeholder=\"请再次输入密码\" v-model=\"checkPsw\" @keyup.enter=\"submit()\">\r\n          </div>\r\n          <div class=\"container\">\r\n              <button @click=\"submit()\">注册</button>\r\n          </div>\r\n      </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\nname:'Register',\r\ndata(){\r\n    return{\r\n        isOK:false,\r\n        select:Number,\r\n        username:'',\r\n        password:'',\r\n        checkPsw:'',\r\n        usnOK:true,\r\n        pswOK:true,\r\n        cpswOK:true\r\n    }\r\n},\r\nmethods:{\r\n    submit:async function(){\r\n        this.checkUp()\r\n        if(this.isOK){\r\n            const result=await this.$http({\r\n                method:'POST',\r\n                url: '/user/register',\r\n                params:{\r\n                    userName:this.username,\r\n                    userPwd:this.password\r\n                }\r\n            })\r\n            if(result.status==200){\r\n                this.$router.push({path:'/login'})\r\n            }\r\n            else{\r\n                this.$bus.$emit('RegisterFail',result.message)\r\n            }\r\n        }\r\n    },\r\n    checkUp:function(){\r\n        this.usnOK=this.username===''?false:true\r\n        this.pswOK=this.password===''?false:true\r\n        this.eMailOK=this.eMail===''?false:true\r\n        this.cpswOK=this.checkPsw===''||this.password!=this.checkPsw?false:true\r\n        if(this.eMailOK&&this.usnOK&&this.pswOK&&this.cpswOK)\r\n            this.isOK=true\r\n        else\r\n            this.isOK=false\r\n    }\r\n}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#root{\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.form{\r\n    display: flex;\r\n    flex-direction: column;\r\n    width: 400px;\r\n    height: 500px;\r\n    background-color: #fff;\r\n    border-radius: 30px;\r\n    box-shadow:  \r\n    5px 5px 10px rgba(0, 0, 0, .2) ;\r\n    transition: .1s;\r\n    z-index: 2;\r\n}\r\n.form:hover{\r\n    transform: scale(1.05);\r\n    box-shadow: 5px 5px 20px rgba(0, 0, 0, .1),\r\n    -5px -5px 10px rgba(225, 225, 225, .1) ;\r\n}\r\n.form p{\r\n    width: 100%;\r\n    font-size: 1.8em;\r\n    font-weight: bold;\r\n    margin-top:40px;\r\n}\r\n.jump{\r\n    margin-top: 10px;\r\n    margin-bottom: 20px;\r\n}\r\n.container{\r\n    display: inherit;\r\n    justify-content: space-around;\r\n    width: 100%;\r\n    margin-bottom: 30px;\r\n}\r\n.container span{\r\n    width: 80px;\r\n    height: 100%;\r\n    line-height: 40px;\r\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\r\n    font-size: 1em;\r\n    color: #555555;\r\n}\r\n.container input,\r\n.container select{\r\n    height: 40px;\r\n    width: 200px;\r\n    color: #333333;\r\n    font-size: 0.9em;\r\n    outline: none;\r\n    border-style: none;\r\n}\r\n.container button{\r\n    width: 150px;\r\n    height: 50px;\r\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\r\n    font-size: 1em;\r\n    font-weight: 600;\r\n    outline: none;\r\n    background-color: #42A5F5;\r\n    color: #fff;\r\n    border-style: none;\r\n    border-radius: 10px;\r\n}\r\n.container button:hover{\r\n    cursor: pointer;\r\n\r\n}\r\n.container button:active{\r\n    background-color: #1976D2;\r\n}\r\n.warn{\r\n    color:red !important\r\n}\r\n</style>"],"sourceRoot":"src/pages"}]}