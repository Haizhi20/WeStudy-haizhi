A,WAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,KAHA;AAIA,SAAA,IAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AACA,KAHA;AAIA,SAAA,IAAA,CAAA,GAAA,CAAA,cAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,KAHA;AAIA,GArCA;AAqCA,EAAA,aArCA,2BAqCA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,MAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,iBAAA;AACA,IAAA,YAAA,CAAA,KAAA,IAAA,CAAA;AACA;AAzCA,CAAA","sourcesContent":["<template>\r\n<transition name=\"fade\">\r\n  <div id=\"alert\" :class=\"isRed==true?'red':'green'\" v-show=\"isShow\">\r\n    {{message+'!'}}\r\n  </div>\r\n</transition>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name:'Alert',\r\n  data(){\r\n    return {\r\n      message:'',\r\n      isShow:false,\r\n      isRed:true,\r\n      time:null\r\n    }\r\n  },methods: {\r\n    revoke(){\r\n      this.message=''\r\n      this.isShow=false\r\n      window.clearTimeout(this.time)\r\n      this.time=null\r\n      console.log('计时器回调执行完毕',this.time)\r\n    },\r\n    show(message){\r\n      this.message=message\r\n      this.isShow=true\r\n      console.log('消息收到了:',this.message,this.isShow)\r\n      this.time=window.setTimeout(()=>{this.revoke()},3000)//解决setTimeout被立即执行的的bug\r\n    }\r\n  },\r\n  mounted(){\r\n    this.$bus.$on('LoginFail',(msg)=>{\r\n      this.show(msg)\r\n      this.isRed=true\r\n    })\r\n    this.$bus.$on('RegisterSuccess',(msg)=>{\r\n      this.show(msg)\r\n      this.isRed=false\r\n    })\r\n    this.$bus.$on('RegisterFail',(msg)=>{\r\n      this.show(msg)\r\n      this.isRed=true\r\n    })\r\n  },beforeDestroy(){\r\n    this.$bus.$off(\"Fail\")\r\n    this.$bus.$off(\"RegisterSuccess\")\r\n    clearTimeout(this.time)\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#alert{\r\n  position: absolute;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 1em;\r\n  font-weight: 500;\r\n  top: 100px;\r\n  right: -150px;\r\n  width: 300px;\r\n  height: 60px;\r\n  border-radius: 10px;\r\n  background-color: #fff;\r\n}\r\n.red{\r\n  color: #ff0026;\r\n  border: solid 2px #ff0026;\r\n}\r\n.green{\r\n  color: rgb(0, 255, 0);\r\n  border: solid 2px rgb(0, 255, 0)!important;\r\n}\r\n.fade-enter,.fade-leave-to{\r\n  opacity: 0;\r\n}\r\n.fade-leave,.fade-enter-to{\r\n  opacity: 1;\r\n}\r\n.fade-enter-active,.fade-leave-active{\r\n  transition: all .3s;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}